{
  "name": "sass-made-fun",
  "version": "1.0.0",
  "description": "Re-create starbucks landing page with HTML, CSS/SASS, and a bit of Javascript",
  "main": "index.js",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "parcel ./src/index.html",
    "prebuild": "shx rm -rf dist/*",
    "build": "parcel build ./src/index.html --no-minify --public-url ./",
    "push-gh-pages": "push-dir --dir=dist --branch=gh-pages --cleanup --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/janevalencia/sass-made-fun.git"
  },
  "keywords": [],
  "author": "Jane Valencia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/janevalencia/sass-made-fun/issues"
  },
  "homepage": "https://github.com/janevalencia/sass-made-fun#readme",
  "devDependencies": {
    "parcel-bundler": "^1.3.1",
    "push-dir": "^0.4.1",
    "sass": "^1.55.0",
    "shx": "^0.3.4"
  }
}
